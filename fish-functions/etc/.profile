# Add some easy shortcuts for directory listings and add a touch of color.
# ls options:
# -h: human-readable units
# -F: Add '*' for executables, '@' for symlinks, '/' for directories, etc.
# -G: Add color output
alias la='ls -halFG'

export JAVA_HOME=/root/tyler/jdk-11.0.7+10
export PATH=$HOME/bin:$JAVA_HOME/bin:/root/tyler/apache-maven-3.6.3/bin:$PATH
export USERNAME=fritz

